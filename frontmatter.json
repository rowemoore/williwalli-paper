{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.id": "astro",
  "frontMatter.content.publicFolder": "public",
  "frontMatter.preview.host": "http://localhost:3000",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "pubDatetime",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Author",
          "name": "author",
          "type": "string",
          "single": true
        },
        {
          "title": "Post slug",
          "name": "postSlug",
          "type": "slug"
        },
        {
          "title": "Featured",
          "name": "featured",
          "type": "boolean"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "boolean"
        },
        {
          "title": "OG Image",
          "name": "ogImage",
          "type": "image",
          "isPreviewImage": true
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        }
      ]
    }
  ],
  "frontMatter.content.pageFolders": [
    {
      "title": "blog",
      "path": "[[workspace]]/src/content/blog"
    }
  ],
  "frontMatter.taxonomy.tags": [
    "Astro",
    "TypeScript",
    "astro-paper",
    "color-schemes",
    "configuration",
    "docs",
    "forestry-cms",
    "release"
  ],
  "frontMatter.taxonomy.categories": []
}